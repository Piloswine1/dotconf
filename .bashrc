# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

[ -f ~/.config/bash_completion.d/.fzf.bash ] && source ~/.config/bash_completion.d/.fzf.bash
[ -f ~/.config/bash_completion.d/git-completion.bash ] && source ~/.config/bash_completion.d/git-completion.bash
[ -f ~/.config/nnn/quitcd.bash ] && source ~/.config/nnn/quitcd.bash

[ -f ~/.config/bash_completion.d/deno.bash ] && source ~/.config/bash_completion.d/deno.bash

